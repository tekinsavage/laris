{
  "name": "laris/cli",
  "description": "A supercharged Laravel development CLI tool for faster workflow",
  "keywords": ["laravel", "laris", "cli", "tool"],
  "license": "MIT",
  "authors": [
    {
      "name": "itarrshia",
      "email": "itarrshia@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/filesystem": "^10.20|^11.0|^12.0",
    "illuminate/support": "^10.20|^11.0|^12.0",
    "laravel/prompts": "^0.1.18|^0.2.0|^0.3.0",
    "symfony/console": "^6.2|^7.0",
    "symfony/polyfill-mbstring": "^1.31",
    "symfony/process": "^6.2|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.1",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "Laris\\": "src/"
    }
  },
  "bin": ["bin/laris"],
    "config": {
        "sort-packages": true,
    "process-timeout": 1100000
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
